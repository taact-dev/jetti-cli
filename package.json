{
  "name": "@jetti/jetti.cli",
  "description": "Jetti CLI",
  "version": "0.0.15",
  "author": "Jon Bolt @epicbagel",
  "bin": {
    "jetti": "./bin/run"
  },
  "bugs": "https://github.com/taact-dev/jetti.cli/issues",
  "dependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-do-expressions": "^7.6.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "7.3.2",
    "@babel/plugin-proposal-optional-chaining": "7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.3.2",
    "@babel/plugin-syntax-throw-expressions": "^7.2.0",
    "@babel/plugin-transform-destructuring": "7.3.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "7.3.1",
    "@babel/register": "^7.0.0",
    "@jetti/eslint-config-jetti": "^0.1.1",
    "@jetti/jetti.problem": "0.1.2",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "auto-changelog": "^2.2.1",
    "bluebird": "^3.7.2",
    "bottleneck": "^2.19.5",
    "build-url": "^6.0.1",
    "cli-ux": "^5.5.1",
    "commitizen": "^4.2.3",
    "dotenv": "^8.2.0",
    "flat": "^5.0.2",
    "got": "^11.8.1",
    "jsonfile": "^6.1.0",
    "lodash": "^4.17.20",
    "open": "^7.3.1",
    "prettyjson": "^1.2.1",
    "qs": "^6.9.6",
    "standard-version": "^9.1.0",
    "underscore.string": "^3.3.5",
    "url-join": "^4.0.1",
    "urljoin": "^0.1.5",
    "write-yaml": "^1.0.0",
    "yaml-reader": "^1.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "chai": "^4",
    "cz-conventional-changelog": "3.3.0",
    "globby": "^10",
    "husky": "^4.3.8",
    "mocha": "^5",
    "nyc": "^14"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/taact-dev/jetti.cli",
  "keywords": [
    "jetti.cli"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "jetti",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "taact-dev/jetti.cli",
  "scripts": {
    "start": "node ./bin/run",
    "build:lint": "eslint './src/**/*.js'",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md",
    "release": "cz && standard-version"
  }
}
